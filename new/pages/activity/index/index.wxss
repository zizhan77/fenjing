  z-index: 1;
  background: #9fe8ff;
}

.spons_k image {
  display: block;
  max-width: 96%;
  max-height: 96%;
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  bottom: 0;
  margin: auto;
}

.start_but {
  width: 600rpx;
  height: 94rpx;
  line-height: 80rpx;
  border: 4rpx solid #202020;
  border-radius: 45rpx;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 30rpx;
  margin: auto;
  background: #9fe8ff;
}

.start_but p {
  position: relative;
  display: block;
  width: 100%;
  text-align: center;
  font-size: 32rpx;
  color: #202020;
  line-height: 72rpx;
  z-index: 10;
  font-weight: bold;
}

.start_but:after {
  content: "";
  display: block;
  width: 600rpx;
  height: 80rpx;
  box-sizing: border-box;
  border: 4rpx solid #202020;
  border-radius: 40rpx;
  position: absolute;
  z-index: 3;
  left: -4rpx;
  top: -4rpx;
  background: #fff388;
}

.start_con {
  width: 100%;
  text-align: Center;
  opacity: 0.8;
  font-size: 26rpx;
  color: #202020;
  height: 37rpx;
  line-height: 37rpx;
  position: absolute;
  top: -67rpx;
  left: -4rpx;
}

button {
