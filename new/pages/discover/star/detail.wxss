/* pages/discover/star/detail.wxss */
@import "../../../wxParse/wxParse.wxss";
page{background-image:none;}
.mine_bar{background-image:None;}
.backimage{position:absolute;width:750rpx;height:403rpx;left:0;top:0;z-index:0;filter:blur(10px) brightness(60%);}
.star_tit{width:690rpx;height:180rpx;margin:10rpx 30rpx 30rpx;position:relative;z-index:10;display:flex;align-items:center;}
.star_toux{width:180rpx;height:180rpx;margin-right:30rpx;position:realtive;}
.star_toux image{width:180rpx;height:180rpx;border-radius:10rpx;overflow:hidden;}
.star_text{flex:1 1 50%;height:180rpx;width:100%;color:#fff;text-align:left;}
.star_textit{height:45rpx;line-height:45rpx;font-size:32rpx;}
.star_textp{height:32rpx;line-height:32rpx;font-size:28rpx;margin:5rpx 0 18rpx;}
.star_textp p{display:inline-block;width:28rpx;height:28rpx;vertical-align: middle;background-image:url('data:image/png;base64,');background-size:100% 100%;}
.star_but {
  width: 100%;
  height: 77rpx;
  border: 1rpx solid #202020;
  margin: 4rpx auto;
  border-radius: 43rpx;
  background: #9fe8ff;
  line-height: 60rpx;
  font-size: 28rpx;
  color: #202020;
  float: right;
  position: relative;
  text-aligN: center;
  font-weight: bold;
}

.star_but p {
  position: relative;
  z-index: 5;
}
.star_but:after {
  content: "";
  display: block;
  width: 100%;
  height: 70rpx;
  border: 1rpx solid #202020;
  border-radius: 40rpx;
  background: #fff388;
  position: absolute;
  left: -1rpx;
  top: -4rpx;
  z-index: 0;
  box-sizing: border-box;
}

.star_butfree {
  width: 100%;
  height: 85rpx;
  border: 3rpx solid #202020;
  margin: 31rpx auto;
  border-radius: 44rpx;
  background: #fff388;
  line-height: 68rpx;
  font-size: 30rpx;
  color: #202020;
  position: relative;
  text-aligN: center;
  font-weight: bold;
}

.star_butfree p {
  position: relative;
  z-index: 5;
}
.star_butfree:after {
  content: "";
  display: block;
  width: 100%;
  height: 78rpx;
  border: 3rpx solid #202020;
  border-radius: 40rpx;
  background: #fff url(http://resource.jiyilive.com/img2/sign/gezi.png) 0 0 repeat;
  position: absolute;
  left: -1rpx;
  top: -4rpx;
  z-index: 0;
  box-sizing: border-box;
}

/* 标签样式 */
.spon_t1{background:#FF3E25;}
