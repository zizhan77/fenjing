.star_con{width:100%;position:relative;z-index:10;border-top-left-radius:20rpx;border-top-right-radius:20rpx;background:#fff;box-sizing:border-box;padding:30rpx 30rpx;}
.no_datas{margin:30px 0;text-align:Center;font-size:28rpx;color:#C49DB6;}

/* 行程样式 */
.star_menuk{width:690rpx;height:60rpx;}
.star_menu{width:auto;height:60rpx;white-space: nowrap;min-width:690rpx;}
.star_m{display:inline-block;width:160rpx;height:37rpx;margin:0 52rpx;line-height:37rpx;font-size:26rpx;color:#666;text-align:Center;border-radius:19rpx;box-sizing:border-box;}
.star_m:first-child{margin-left:0;}
.star_m:last-child{margin-right:0;}
.star_mac{border:2rpx solid #202020;color:#202020 !important;background:#fff388;position:relative;line-height:33rpx;overflow:visible;}
.star_mac:after{
  content:'';
  display:block;
  position:absolute;
  z-index:10;
  left:50%;bottom:-7.5rpx;
  margin-left:-7rpx;
  width:10rpx;height:10rpx;
  border:2rpx solid #202020;
  border-left:none;border-top:none; 
  transform:rotate(45deg);
  background:linear-gradient(135deg,transparent 0%,transparent 50%,#fff388 50%,#fff388 100%);
}

/* 行程线 */
.star_trip{width:660rpx;margin-left:30rpx;border-left:2rpx solid #ccc;box-sizing:border-box;color:#ccc;padding:20rpx 0 20rpx 0;overflow:visible;}
.star_tripd{width:100%;padding-left:60rpx;box-sizing:border-box;position:relative;margin:28rpx 0 32rpx;overflow:visible;}
.star_tripd:before{content:'';display:block;width:20rpx;height:20rpx;border-radius:50%;border:2rpx solid #ccc;position:absolute;left:-10rpx;top:10rpx;background:#fff;z-index:10;}
.star_trtit{line-height:50rpx;font-size:32rpx;font-weight:bold;}
.star_trtime{height:37rpx;line-height:37rpx;font-size:26rpx;margin:5rpx;}
.star_trtbut{width:136rpx;height:37rpx;line-height:34rpx;border-radius:20rpx;font-size:24rpx;text-align:center;float:right;border:2rpx solid #ccc;}
.star_over:after{content:"活动结束";}
.star_run:after{content:"参与活动";}
.star_tripdzz{position:absolute;left:0;top:0;right:0;bottom:0;background:rgba(0,0,0,0);z-index:15;}

.trp1,.trp2,.trp3{color:#202020;}
