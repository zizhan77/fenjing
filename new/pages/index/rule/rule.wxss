/* pages/index/rule/rule.wxss */
@import "/wxParse/wxParse.wxss";
.backk{width:100%;background:#fff;overflow-x:hidden;line-height:0;}
.backk view image{width:100%;margin:0;display:block;overflow:HIdden;}

page{padding-bottom:100rpx;background-image:none;}
.rulebut_k{position:fixed;width:100%;height:100rpx;background:#fff;left:0;bottom:0;z-index:999;line-height:100rpx;text-align:center;padding:5rpx 0;}
.rulebut_i{height:90rpx;width:70%;position:relative;}
.taped{position:absolute;width:50rpx;height:50rpx;left:50%;top:10rpx;margin-left:-130rpx;z-index:99;animation:tand 3s ease infinite;}
@keyframes tand{
  15%{transform:scale(1,1);}
  20%{transform:scale(1.2,1.2);}
  25%{transform:scale(1,1);}
  30%{transform:scale(1.2,1.2);}
  35%{transform:scale(1,1);}
  55%{transform:scale(1.2,1.2);}
  60%{transform:scale(1,1);}
  65%{transform:scale(1.2,1.2);}
  70%{transform:scale(1,1);}
}
.ract_but{width:690rpx;margin:7rpx auto;box-sizing:border-box;line-height:68rpx;border:2rpx solid #231916;border-radius:36rpx;background:#9fe8ff;text-align:center;position:relative;height:80rpx;}
.ract_but p{display:block;position:relative;z-index:10;color:#231916;font-size:28rpx;font-weight:bold;}
.ract_but:after{content:"";display:block;position:absolute;left:-2rpx;top:-2rpx;width:690rpx;border:2rpx solid #231916;height:70rpx;border-radius:35rpx;background:#fff388;box-sizing:border-box;}


