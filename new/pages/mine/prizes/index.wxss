.nav_dhd {
  flex: 0 0 160rpx;
  text-align: center;
  font-size: 30rpx;
  color: #666;
  line-height: 45rpx;
  height: 58rpx;
  white-space: Nowrap;
}

.nav_dhdac {
  color: #202020 !important;
  position: relative;
  overflow: Hidden;
  font-weight: bold;
}

.nav_dhdac:after {
  content: "";
  display: block;
  position: absolute;
  height: 16rpx;
  width: 38rpx;
  border-radius: 8rpx;
  bottom: -8rpx;
  left: 50%;
  margin-left: -19rpx;
  background: #ff9fca;
}

.nav_lik {
  width: 100%;
  min-height: 375rpx;
  position: relative;
  top: 44rpx;
}

.nav_li {
  width: 100%;
  box-sizing: border-box;
  line-height: 45rpx;
  margin: 38rpx 0 29rpx;
  overflow: Hidden;
  text-overflow: ellipsis;
  font-size: 32rx;
  color: #202020;
  font-weight: bold;
  position: relative;
  padding: 0 0 15rpx 120rpx;
}

.nav_lif {
  width: 80rpx;
  height: 80rpx;
  float: left;
  margin-right: 30rpx;
  position: absolute;
  left: 20rpx;
  top: 20rpx;
}

.li_tit {
  max-height: 90rpx;
  min-height: 45rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  line-clamp: 2;
  display: -webkit-box;
  text-overflow: -o-ellipsis-lastline;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.nav_lilo {
  width: 80rpx;
  height: 80rpx;
  float: left;
  margin-right: 30rpx;
  box-sizing: border-box;
  border: 2rpx solid #202020;
  border-radius: 15rpx;
  position: relative;
  left: 0rpx;
  top: 0rpx;
}

.nav_li span {
  font-size: 26rpx;
  color: #666;
  display: block;
  width: 100%;
  font-weight: normal;
}

.nav_li b {
  color: #ff3e25;
  display: inline;
  word-break: break-all;
}

.nav_lico {
  width: auto;
  text-align: left;
  height: 74rpx;
  min-height: 74rpx;
  box-sizing: border-box;
  position: relative;
  line-height: 37rpx;
  font-size: 26rpx;
  color: #666;
  margin-bottom: 32rpx;
  overflow: Hidden;
  left: 0rpx;
  top: 0rpx;
  margin-right: 20rpx;
}

.nav_liex {
  position: absolute;
  width: 82rpx;
  height: 37rpx;
  line-height: 37rpx;
  right: 0;
  bottom: -1rpx;
  text-align: right;
  color: #9fe8ff;
  z-index: 10;
  background: linear-gradient(90deg, rgba(255, 255, 255, 0) 0%, #fff 33%, #fff 100%);
}
.nav_copy {
  width: auto;
  height: auto;
  border: 2rpx solid #202020;
  border-radius: 40rpx;
  box-sizing: border-box;
  text-align: center;
  line-height: 40rpx;
  font-size: 24rpx;
  color: #202020;
  background: #fff388;
  float: right;
  padding: 5rpx 10rpx;
  margin-top:30rpx;
  margin-right:30rpx;
}
